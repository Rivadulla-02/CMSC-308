using System;

using System.IO;

using System.Linq;

using System.Collections.Generic;

namespace CSharp_Shell

{

    public class Program 

    {

        public static void Main()

        {

			Console.Write("Enter name: ");

			String name = Console.ReadLine();

            Console.Write("Enter English Grade: ");

			double eng = Convert.ToDouble (Console.ReadLine());

            Console.Write("Enter Math Grade: ");

			double math = Convert.ToDouble (Console.ReadLine());

            Console.Write("Enter Science Grade: ");

			double sci = Convert.ToDouble (Console.ReadLine());

            Console.Write("Enter Mapeh Grade: ");

			double map = Convert.ToDouble (Console.ReadLine());

           

            double average = (eng + math + sci + map) / 4;

            

            if(average > 75)

            {

            	Console.WriteLine("The student passed!");

            	

            }

            else 

            {

            	Console.WriteLine("The student failed");

            }

            

            Console.WriteLine("The General Average of" + name + "is" + average);

            }

          

        }

    }
